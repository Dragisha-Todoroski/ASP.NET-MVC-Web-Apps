@model List<PizzaViewModelHomework>

<ul>
    @for (int i = 0; i < Model.Count; i++)
    {
        <li>
            <h3>@Model.ElementAt(i).Name</h3>

            <p>Id: @Model.ElementAt(i).Id</p>
            <p>Price: @Model.ElementAt(i).Price</p>
            <p>Size: @Model.ElementAt(i).PizzaSizeHomework</p>
        </li>
    }
</ul>